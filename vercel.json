{
  "version": 2,
  "builds": [
    {
      "src": "/src/index.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "regions": ["sin1"],
  "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
  "headers": {
    "Access-Control-Allow-Origin": "*"
  }
}
